      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme-to-json": "3.4.0",
    "redux-mock-store": "1.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/*.js",
      "!src/components/Root.js",
      "!src/validations/*.js",
      "!src/propTypes/*.js",
      "!src/__fixtures__/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 7,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
