{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["import"],
  "rules": {
    "no-underscore-dangle": 0,
    "linebreak-style": 0,
    "no-shadow": 0,
    "no-nested-ternary": 0
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "document": true,
    "window": true
  }
}
